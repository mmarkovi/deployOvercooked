  <body>
    <form>
      <div class="container">
      <div class="row text-center">
	<div class="form-group col-lg-2" style="visibility: hidden;">
	  <label for="playerZero" style="color:#1E6A9E">Player 1</label>
	  <select class="form-control" id="playerZero">
      <!-- EDIT REMOVE OPTIONS -->
	    <!-- <option value="ppo_bc">Human-aware PPO agent</option>
	    <option value="ppo_sp">Self-play agent</option>
	    <option value="pbt">Population Based Training agent</option> -->
      <option value="human">Human Keyboard Input</option>
	  </select>
	</div>
  <div class="form-group col-lg-2" style="visibility: hidden;">
    <label for="playerOne" style="color:#44956B">Player 2</label>
    <select class="form-control" id="playerOne">
      <!-- EDIT REMOVE OPTIONS -->
      <!-- <option value="human">Human Keyboard Input</option>
      <option value="ppo_bc">Human-aware PPO agent</option> -->
      <option value="ppo_sp">Self-play agent</option>
      <!-- <option value="pbt">Population Based Training agent</option> -->
      
    </select>
  </div>
	<div class="form-group col-lg-2">
	  <label for="layout">Layout</label>
	  <select class="form-control" id="layout">
      <!-- EDIT REMOVE OPTIONS -->
	    <option value="room_1">Room 1</option>
      <option value="room_2">Room 2</option>
      <option value="room_3">Room 3</option>
      <option value="room_4">Room 4</option>
      <option value="room_5">Room 5</option>
      <option value="room_6">Room 6</option>
      <option value="room_7">Room 7</option>
      <option value="room_8">Room 8</option>
      <option value="room_9">Room 9</option>
      <option value="room_10">Room 10</option>
	    <!-- <option value="asymmetric_advantages">Asymmetric Advantages</option>
	    <option value="coordination_ring">Coordination Ring</option>
	    <option value="forced_coordination">Forced Coordination</option>
	    <option value="counter_circuit">Counter Circuit</option> -->
	  </select>
	</div>
  <div class="form-group col-lg-2" style="visibility: hidden;">
    <label for="gameTime">Game Length (sec)</label>
    <input type="number" id="gameTime" value="30" min="1" max="1800">
  </div>
  <div class="form-group col-lg-2" style="visibility: hidden;">
    <label for="layout">Deterministic?</label>
    <input type="checkbox" id="deterministic" name="deterministic">
  </div>
  <div class="form-group col-lg-2">
    <label for="layout">Automatically Save Game Trajectories?</label>
    <input type="checkbox" id="saveTrajectories" name="saveTrajectories">
  </div>
  
      </div>
      </div>
    </form>
    <h4 class="text-center"><a href="/instructions">Instructions</a></h4><h4 class="text-center"><a href="/replay">Replay Trajectories</a></h4> 
